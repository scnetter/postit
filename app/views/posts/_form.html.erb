<h4><%= title %></h4>

<%= render '../shared/error', obj: @post %>

<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br />
  <%= f.submit "Submit", class: 'btn btn-primary' %>
<% end %>
<%= render 'shared/content_title', title: 'New Category' %>
<%= render 'shared/errors', obj: @category %>
<div class'well' >
  <%= form_for @category do |f| %>
    <div class='control-group'>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class='control-group'>
      <%= f.submit "Create Category", class: 'btn btn-success btn-small' %>
    </div>
  <% end %>
</div>